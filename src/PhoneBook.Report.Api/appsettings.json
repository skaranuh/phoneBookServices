{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "PhoneBookReportConnection": "Server=report_db; port=5432; user id=phonebookreportuser; password=phoneBookReportPass; database=phonebookreport; pooling=true"
  },
  "Kafka": {
    "BootstrapServers": "kafka:9092",
    "GroupId": "group1"
  },
  "QueueCapacity": 100,
  "Messaging": {
    "Topic": "report-request"
  },
  "Report":{
    "BaseUrl":"http://phonebookapi:5000/api/phoneBook",
    "Resource": "report",
    "PageSize":100,
    "Path"   :"/report"
  } 
}